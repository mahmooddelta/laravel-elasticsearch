{
    "name": "afzali/laravel-elasticsearch",
    "description": "By using this package, you can seamlessly incorporate Elasticsearch functionality into your Laravel project. It provides a set of Laravel-specific APIs and utilities that simplify the process of interacting with Elasticsearch.",
    "type": "library",
    "keywords": [
        "laravel",
        "elasticsearch",
        "search",
        "client"
    ],
    "require": {
        "php": "^8.1",
        "elasticsearch/elasticsearch": "8.11",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0",
        "illuminate/pagination": "^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "dev-master",
        "phpunit/phpunit": "^9.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Afzali\\LaravelElasticsearch\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mahmood Afzali",
            "email": "mahmooddelta01@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
